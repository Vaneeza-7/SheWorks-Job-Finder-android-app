<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AlertDialogCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">#97D81B5B</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:background">@color/iconSelectedColor</item>
        <item name="android:padding">2dp</item>

    </style>

    <style name="Chip_Custom" parent="Widget.Material3.Chip.Filter">
        <item name="android:backgroundTint">@color/iconSelectedColor</item>
        <item name="android:strokeColor">@color/colorAccent</item>
        <item name="chipBackgroundColor">@color/white</item>
        <item name="chipSurfaceColor">@color/white</item>
        <item name="chipStrokeColor">@color/colorAccent</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>