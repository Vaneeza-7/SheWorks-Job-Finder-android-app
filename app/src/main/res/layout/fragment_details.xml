        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/baseline_edit_24"
        android:background="@color/white"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@id/interestsTitle"
        android:layout_alignBottom="@id/interestsTitle"
        android:padding="10dp"
        android:layout_below="@+id/experienceText"
        />

    <TextView
        android:id="@+id/interestsText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/interestsTitle"
        android:layout_gravity="left"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:padding="10dp"
        android:text="Artificial Intelligence, Machine Learning, Data Science, Software Development"
        android:textAppearance="@style/TextAppearance.Material3.BodyMedium"
        android:textColor="@color/black" />


    <TextView
        android:id="@+id/projectsTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/interestsText"
